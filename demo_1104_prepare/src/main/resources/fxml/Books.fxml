<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane maxHeight="1.7976931348623157E308"
	maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<BorderPane maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<bottom>
				<ButtonBar maxWidth="1.7976931348623157E308"
					minHeight="0.0" minWidth="0.0" BorderPane.alignment="CENTER">
					<buttons>
						<Button fx:id="deleteButton" mnemonicParsing="false"
							onAction="#onDeleteButtonClick" text="%delete" />
					</buttons>
					<BorderPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</BorderPane.margin>
				</ButtonBar>
			</bottom>
			<center>
				<TableView fx:id="table"
					maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0"
					BorderPane.alignment="CENTER">
					<columns>
						<TableColumn editable="false" prefWidth="246.0"
							text="%author">
							<cellValueFactory>
								<PropertyValueFactory property="authorName" />
							</cellValueFactory>
						</TableColumn>

						<TableColumn editable="false" prefWidth="272.0"
							text="%title">
							<cellValueFactory>
								<PropertyValueFactory property="bookTitle" />
							</cellValueFactory>
						</TableColumn>

					</columns>
				</TableView>
			</center>
		</BorderPane>
	</children>
</AnchorPane>
